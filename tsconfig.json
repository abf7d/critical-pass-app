/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "paths": {
      "@critical-pass/auth": [
        "./dist/auth",
        "./projects/libs/auth/src"
      ],
      "@critical-pass/charts": [
        "./projects/libs/charts/src"
      ],
      "@critical-pass/core": [
        "./dist/core","./projects/libs/core/src"
      ],
      "@critical-pass/shared/data-access":  [
        "./projects/libs/shared/data-access/src"
      ],
      "@critical-pass/desktop-lib": [
        "./dist/desktop-lib",
        "./projects/libs/app-libs/desktop-lib/src"
      ],
      "@critical-pass/shared/environments": [
        "./projects/libs/shared/environments/src"
      ],
      "@critical-pass/shared/file-management": [
        "./projects/libs/shared/file-management/src"
      ],
      "@critical-pass/features/history": [
        "./dist/history",
        "./projects/libs/features/history/src"
      ],
      "@critical-pass/features/jira": [
        "./dist/jira",
        "./projects/libs/features/jira/src"
      ],
      "@critical-pass/features/landing": [
        "./dist/landing",
        "./projects/libs/features/landing/src"
      ],
      "@critical-pass/shared/layout": [
        "./dist/layout",
        "./projects/libs/shared/layout/src"
      ],
      "@critical-pass/features/library": [
        "./dist/library",
        "./projects/libs/features/library/src"
      ],
      "@critical-pass/features/network": [
        "./dist/network",
        "./projects/libs/features/network/src"
      ],
      "@critical-pass/project/processor": [
        "./dist/processor",
        "./projects/libs/project/processor/src"
      ],
      "@critical-pass/features/profile": [
        "./dist/profile",
        "./projects/libs/features/profile/src"
      ],
      "@critical-pass/shared/project-utils": [
        "./dist/project-utils",
        "./projects/libs/shared/project-utils/src"
      ],
      "@critical-pass/features/resources": [
        "./dist/resources",
        "./projects/libs/features/resources/src"
      ],
      "@critical-pass/shared/serializers": [
        "./dist/serializers",
        "./projects/libs/shared/serializers/src"
      ],
      "@critical-pass/features/spreadsheet": [
        "./dist/spreadsheet",
        "./projects/libs/features/spreadsheet/src"
        
      ],
      "@critical-pass/project/types": [
        "./projects/libs/project/types/src"
      ],
      "@critical-pass/web-lib": [
        "./projects/libs/app-libs/web-lib/src"
      ],
      "@critical-pass/features/app-data": [
        "./dist/app-data",
        "./projects/libs/features/app-data/src"
      ]
    },
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": [
      "ES2022",
      "dom"
    ],
    "types": ["jasmine", "cypress", "node", "cypress-real-events", "@simonsmith/cypress-image-snapshot/types"]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true,
    "enableIvy": false 
  }
}
