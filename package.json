{
    "name": "critical-pass-app",
    "version": "0.0.0",
    "main": "dist/critical-desktop/electron/apps/critical-desktop/electron/src/main.js",
    "scripts": {
        "ng": "ng",
        "docker-build": "docker build -t critical-pass-app:latest .",
        "visual-test": "sh run_visual_test.sh",
        "remove-test": "docker rm -f test-run",
        "deploy": "ng deploy critical-web --no-nojekyll --dir=dist/critical-web --no-build --cname=app.criticalpass.io",
        "deploy:build": "ng build critical-web --configuration=staging",
        "app-live-docker": "docker run -it --rm -v \"//$(pwd):/app\" -v \"/app/node_modules\" --name test-run critical-pass-app:latest",
        "build:desktop": "ng build --project critical-desktop --configuration production",
        "start": "ng serve --ssl --project=critical-web",
        "start:desktop": "ng serve --project=critical-desktop --port=4201",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "lint": "npm run prettier:check && ng lint",
        "lint:fix": "ng lint --fix --cache && npm run prettier:fix",
        "prettier": "prettier \"**/*.ts\" \"**/*.js\" \"**/*.md\"",
        "prettier:check": "npm run prettier -- -l",
        "prettier:cli": "npm run prettier -- -c",
        "prettier:fix": "npm run prettier:cli -- --write",
        "compile-tsc": "tsc -p tsconfig.electron.json --watch",
        "electron:serve": "electron . --serve",
        "start:electron": "concurrently \"npm run start:desktop\" \"wait-on http://localhost:4201 && npm run compile-tsc\" \"wait-on http://localhost:4201 && npm run electron:serve\"",
        "electron:local": "npm run build:desktop && electron .",
        "postinstall": "husky install"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.0.0",
        "@angular/cdk": "^17.0.1",
        "@angular/common": "^17.0.0",
        "@angular/compiler": "^17.0.0",
        "@angular/core": "^17.0.0",
        "@angular/forms": "^17.0.0",
        "@angular/material": "^17.0.1",
        "@angular/platform-browser": "^17.0.0",
        "@angular/platform-browser-dynamic": "^17.0.0",
        "@angular/platform-server": "^17.0.0",
        "@angular/router": "^17.0.0",
        "@angular/ssr": "^17.0.0",
        "@auth0/angular-jwt": "^5.2.0",
        "@azure/msal-browser": "^2.32.1",
        "@fortawesome/fontawesome-free": "^5.15.4",
        "@microsoft/applicationinsights-angularplugin-js": "^15.3.0",
        "@microsoft/applicationinsights-web": "^3.3.0",
        "ag-grid-angular": "^29.0.0",
        "ag-grid-community": "^29.0.0",
        "bootstrap": "^4.6.2",
        "d3": "^6.7.0",
        "d3-dag": "^1.1.0",
        "d3-regression": "^1.3.10",
        "date-fns": "^2.30.0",
        "express": "^4.18.2",
        "font-awesome": "^4.7.0",
        "ngx-logger": "^5.0.12",
        "ngx-paypal": "^11.0.0",
        "ngx-toastr": "^18.0.0",
        "pako": "^2.1.0",
        "rxjs": "~7.8.0",
        "sqlite3": "^5.1.7",
        "ts-keycode-enum": "^1.0.6",
        "tslib": "^2.3.0",
        "url-join": "^5.0.0",
        "xlsx": "^0.18.5",
        "zone.js": "~0.14.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.0.0",
        "@angular-eslint/builder": "17.1.0",
        "@angular-eslint/eslint-plugin": "17.1.0",
        "@angular-eslint/eslint-plugin-template": "17.1.0",
        "@angular-eslint/schematics": "17.1.0",
        "@angular-eslint/template-parser": "17.1.0",
        "@angular/cli": "^17.0.0",
        "@angular/compiler-cli": "^17.0.0",
        "@commitlint/cli": "^18.4.2",
        "@commitlint/config-conventional": "^18.4.2",
        "@simonsmith/cypress-image-snapshot": "^9.0.1",
        "@types/d3": "^7.4.0",
        "@types/electron": "^1.6.10",
        "@types/express": "^4.17.17",
        "@types/jasmine": "~5.1.0",
        "@types/node": "^18.18.0",
        "@types/pako": "^2.0.0",
        "@types/sqlite3": "^3.1.11",
        "@typescript-eslint/eslint-plugin": "6.11.0",
        "@typescript-eslint/parser": "6.11.0",
        "angular-cli-ghpages": "^2.0.0-beta.2",
        "concurrently": "^8.2.2",
        "cypress": "^13.6.4",
        "cypress-real-events": "^1.12.0",
        "electron": "^29.0.1",
        "electron-builder": "^24.12.0",
        "electron-debug": "^3.2.0",
        "electron-reloader": "^1.2.3",
        "eslint": "^8.53.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "husky": "^8.0.3",
        "jasmine-core": "~5.1.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "lint-staged": "^15.1.0",
        "ng-packagr": "^17.0.0",
        "prettier": "^3.1.0",
        "prettier-eslint": "^16.1.2",
        "ts-node": "^10.9.2",
        "typescript": "~5.2.2",
        "wait-on": "^7.2.0"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,html}": [
            "npm run lint:fix"
        ]
    },
    "engines": {
        "node": ">= 16.14.0 || >= 18.10.0"
    }
}
